<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
    <Dialog id="_IDGenDialog1" title="{i18n>orders}">
        <Table id="_IDGenTable1" items="{odataNorthwind>Orders}" width="700px">
            <columns>
                <Column id="_IDGenColumn1" >
                    <header>
                        <Label id="_IDGenLabel1" text="{i18n>orderID}"/>
                    </header>
                </Column>
                <Column id="_IDGenColumn2" >
                    <header>
                        <Label id="_IDGenLabel4" text="{i18n>freight}"/>
                    </header>
                </Column>
                <Column id="_IDGenColumn3" >
                    <header>
                        <Label id="_IDGenLabel5" text="{i18n>shipAddress}"/>
                    </header>
                </Column>
            </columns>
            <items>
                <ColumnListItem id="_IDGenColumnListItem1" >
                    <cells>
                        <Label id="_IDGenLabel6" text="{odataNorthwind>OrderID}"/>
                        <Label id="_IDGenLabel7" text="{odataNorthwind>Freight}"/>
                        <Label id="_IDGenLabel8" text="{odataNorthwind>ShipAddress}"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
        <endButton>
            <Button id="_IDGenButton1" text="{i18n>close}" press="onCloseOrders"></Button>
        </endButton>
    </Dialog>
</core:FragmentDefinition>